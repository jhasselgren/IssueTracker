
.panel.panel-default
	.panel-body
		a(ng-href="#/decision/show/{{ctrl.data.decisionId}}")
			h4.list-group-item-heading {{ctrl.data.decisionId}} - {{ctrl.data.title}}
		p
			span.label.label-success Completed
			span.label.label-default Solution
			span.label.label-default Change Control Board

		p.list-group-item-text 
			{{ctrl.data.description}}